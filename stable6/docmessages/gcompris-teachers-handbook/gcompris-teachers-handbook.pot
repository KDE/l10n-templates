#, no-c-format
msgid "Overview"
msgstr ""

#. Tag: para
#: index.docbook:75
#, no-c-format
msgid "The goal of gcompris-teachers is to enable teachers to:"
msgstr ""

#. Tag: para
#: index.docbook:79
#, no-c-format
msgid "create and manage pupils groups."
msgstr ""

#. Tag: para
#: index.docbook:80
#, no-c-format
msgid "create specific work plans."
msgstr ""

#. Tag: para
#: index.docbook:81
#, no-c-format
msgid "connect and send a work plan to pupils."
msgstr ""

#. Tag: para
#: index.docbook:82
#, no-c-format
msgid "visualize the work results per group and pupil."
msgstr ""

#. Tag: para
#: index.docbook:83
#, no-c-format
msgid "analyse pupils answers."
msgstr ""

