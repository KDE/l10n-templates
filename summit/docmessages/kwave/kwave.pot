#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6522
#, no-c-format
msgid "&no-i18n-tag;<command>[%nr]</command>"
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6523
#, no-c-format
msgid "Will be replaced with the current index of the file to save."
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6529
#, no-c-format
msgid "&no-i18n-tag;<command>[%count]</command>"
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6530
#, no-c-format
msgid "Will be replaced with the number of sections that will be saved."
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6536
#, no-c-format
msgid "&no-i18n-tag;<command>[%total]</command>"
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6537
#, no-c-format
msgid "Will be replaced with the index of the last file to save."
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6543
#, no-c-format
msgid "&no-i18n-tag;<command>[%filename]</command>"
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6544
#, no-c-format
msgid "Will be replaced with the base file name, without path and without extension."
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6550
#, no-c-format
msgid "&no-i18n-tag;<command>[%fileinfo{<replaceable>keyword</replaceable>}]</command>"
msgstr ""

#. Tag: entry
#. +> trunk6 stable6
#: index.docbook:6551
#, no-c-format
msgid "Will be replaced with the content of a file information identified by <replaceable>keyword</replaceable>. See section <link linkend=\"file-info-list\" endterm=\"file-info-list-title\"></link> for a list of all available keywords."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:6574
#, no-c-format
msgid "All numeric wildcards can also contain a numerical argument after the \"<literal>%</literal>\" and the identifier, to force a certain number of digits. If the number is preceded by a <literal>0</literal> then the result will contain leading zeroes, otherwise it will contain leading spaces."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:6581
#, no-c-format
msgid "Example: <command><literal>[%04nr]</literal></command> produces a number between 0001 and 9999."
msgstr ""

