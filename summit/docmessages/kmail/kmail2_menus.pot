#, no-c-format
msgid "<screeninfo>The View Menu</screeninfo>"
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: menus.docbook:345 menus.docbook:1627
#, no-c-format
msgid "<phrase>The View Menu</phrase>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:348 menus.docbook:1630
#, no-c-format
msgid "<para>The View Menu</para>"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:356
#, no-c-format
msgid "<guimenu>View</guimenu><guisubmenu>Message List &gt;</guisubmenu>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:357
#, no-c-format
msgid "This submenu has the following options to change the appearance of the message list."
msgstr ""

#. Tag: guisubmenu
#. +> trunk6 stable6
#: menus.docbook:361
#, no-c-format
msgid "Sorting &gt;"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:362
#, no-c-format
msgid "Contains options to customize your sort order as follows:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:364
#, no-c-format
msgid "<guilabel>Message Sort Order</guilabel> has several options, most of which are self-explanatory. The following options may not be immediately obvious, and deserve an explanation."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:367
#, no-c-format
msgid "<guilabel>By Smart Sender/Receiver</guilabel> will display the receiver when you are viewing the sent-mail folder, and will display the sender when you are viewing the inbox."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:370
#, no-c-format
msgid "<guilabel>By Action Item Status</guilabel> and <guilabel>By Important Status</guilabel>. These flags need to be set before they can be sorted. Please see <link linkend=\"reader-mark-messages\">Mark Messages</link> for more information on marking your messages."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:376
#, no-c-format
