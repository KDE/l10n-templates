#, no-c-format
msgid "The Game Menu"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:453
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>N</keycap></keycombo></shortcut> <guimenu>Game</guimenu> <guimenuitem>New</guimenuitem>"
msgstr ""

#. Tag: action
#. +> trunk6 stable6
#: index.docbook:458
#, no-c-format
msgid "Starts a new game."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:461
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>O</keycap></keycombo></shortcut> <guimenu>Game</guimenu> <guimenuitem>Load…</guimenuitem>"
msgstr ""

#. Tag: action
#. +> trunk6 stable6
#: index.docbook:466
#, no-c-format
msgid "Loads a saved game."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:469
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>S</keycap></keycombo></shortcut> <guimenu>Game</guimenu> <guimenuitem>Save</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:474
#, no-c-format
msgid "<action>Saves</action> the current game as a default save file."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:477
#, no-c-format
msgid "<guimenu>Game</guimenu> <guimenuitem>Save As…</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:481
#, no-c-format
msgid "<action>Saves</action> the current game as a file of your choice."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:484
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>P</keycap></keycombo></shortcut> <guimenu>Game</guimenu> <guimenuitem>Print…</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:489
#, no-c-format
msgid "<action>Prints</action> the current state of a two-dimensional game on a printer of your choice. The block outlines and original puzzle values are printed in black. Other cells, filled-in values and solution values are printed in gray. There is an option to print just one puzzle or more than one puzzle per page. See <link linkend=\"configuration\">Game Configuration</link>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:499
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>Q</keycap></keycombo></shortcut> <guimenu>Game</guimenu> <guimenuitem>Quit</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:504
#, no-c-format
msgid "<action>Quits</action> &ksudoku;."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:509
#, no-c-format
msgid "The Move Menu"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:512
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;<keycap>Z</keycap></keycombo></shortcut> <guimenu>Move</guimenu> <guimenuitem>Undo</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:517
#, no-c-format
msgid "<action>Undo</action> your last move, repeatedly if required."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:520
#, no-c-format
msgid "<shortcut><keycombo action=\"simul\">&Ctrl;&Shift;<keycap>Z</keycap></keycombo></shortcut> <guimenu>Move</guimenu> <guimenuitem>Redo</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:525
#, no-c-format
msgid "<action>Redo</action> a move that was undone, repeatedly if required."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:528
#, no-c-format
msgid "<shortcut><keycap>H</keycap></shortcut> <guimenu>Move</guimenu> <guimenuitem>Hint</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:533
#, no-c-format
msgid "<action>Give a Hint</action> by filling in a vacant square."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:536
#, no-c-format
msgid "<guimenu>Move</guimenu> <guimenuitem>Solve</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:540
#, no-c-format
msgid "<action>Instantly solve</action> the puzzle."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:543
#, no-c-format
msgid "<guimenu>Move</guimenu> <guimenuitem>Check</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:547
#, no-c-format
msgid "<action>Check</action> an entered puzzle for validity. If it has only one solution, it becomes available for play. If it has no solution or more than one solution, there has probably been an error in the data-entry."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:552
#, no-c-format
msgid "The Settings Menu"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:555
#, no-c-format
msgid "<guimenu>Settings</guimenu> <guimenuitem>Configure &ksudoku;…</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:559
#, no-c-format
msgid "<action>Open a dialog to configure &ksudoku;.</action> See <link linkend=\"configuration\">Game Configuration</link> section for details."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:562
#, no-c-format
msgid "Additionally &ksudoku; has the common &kde; <guimenu>Settings</guimenu> and <guimenu>Help</guimenu> menu items, for more information read the sections about the <ulink url=\"help:/fundamentals/menus.html#menus-settings\">Settings Menu</ulink> and <ulink url=\"help:/fundamentals/menus.html#menus-help\">Help Menu</ulink> of the &kde; Fundamentals."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:571
#, no-c-format
msgid "Frequently asked questions"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:576
#, no-c-format
msgid "I want to change the way this game looks. Can I?"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:577
#, no-c-format
msgid "Sudoku games are traditionally played using a square grid. Themes do not make much sense here, but you can select some color schemes and backgrounds. To do so use <menuchoice><guimenu>Settings</guimenu><guimenuitem>Configure &ksudoku;…</guimenuitem></menuchoice> on the menubar."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:580
#, no-c-format
msgid "Can I use the keyboard to play this game?"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:581
#, no-c-format
msgid "Yes. In &ksudoku; keyboard play is always enabled. Please use the cursor keys to move the selection, numeric or alphabetic characters to enter the symbols and the &Shift; key to apply markers."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:584
#, no-c-format
msgid "Where are the high scores?"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:585
#, no-c-format
msgid "&ksudoku; does not have this feature."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:592
#, no-c-format
msgid "Game Configuration"
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:595
#, no-c-format
msgid "Game Options"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:598
#, no-c-format
msgid "When printing, try to fit more than one puzzle on a page"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:600
#, no-c-format
msgid "Toggle the option to print more than one puzzle per page whenever they will fit."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:601
#, no-c-format
msgid "If this option is checked, puzzles will be printed when no more will fit on a page or when &ksudoku; terminates. Otherwise a puzzle is printed immediately."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:603
#, no-c-format
msgid "Three-dimensional puzzles cannot be printed."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:607
#, no-c-format
msgid "Show Highlights in 2-D puzzles"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:608
#, no-c-format
msgid "Toggle the &ksudoku; highlight option for two-dimensional puzzles."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:611
#, no-c-format
msgid "Show Errors"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:612
#, no-c-format
msgid "Toggle the option to highlight errors."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:615
#, no-c-format
msgid "Show Highlights in 3-D puzzles"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:616
#, no-c-format
msgid "Toggle the &ksudoku; highlight option for three-dimensional puzzles."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:619
#, no-c-format
msgid "Mathdoku puzzle size (3-9)"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:620
#, no-c-format
msgid "Choose a size for Mathdoku puzzles, from 3 (very easy) to 9 (very hard) with size 6 being the default."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:623
#, no-c-format
msgid "Settings for 3-D Puzzles Only"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:624
#, no-c-format
msgid "These five settings adjust the highlighting and visibility of cells in three-dimensional puzzles. The idea is to make it easier to see relationships between cells, especially if they are behind other cells."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:627
#, no-c-format
msgid "Having the highlighting options enabled is recommended even for the most experienced players, as it helps you see the actual game board constraints."
msgstr ""

