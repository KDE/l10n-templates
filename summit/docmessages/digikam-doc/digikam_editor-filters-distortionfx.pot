#. Tag: title
#. +> trunk5
#: editor-filters-distortionfx.docbook:4
#, no-c-format
msgid "Distortion FX"
msgstr ""

#. Tag: para
#. +> trunk5
#: editor-filters-distortionfx.docbook:7
#, no-c-format
msgid "The &digikam; <emphasis>Distortion FX</emphasis> is a series of distorting effects for digital still images."
msgstr ""

#. Tag: title
#. +> trunk5
#: editor-filters-distortionfx.docbook:14
#, no-c-format
msgid "Introduction"
msgstr ""

#. Tag: para
#. +> trunk5
#: editor-filters-distortionfx.docbook:16
#, no-c-format
msgid "With this filter set, you can transform an ordinary photograph into a work of art suitable for framing using distorting operations. It uses algorithms copyrighted by Pieter Voloshyn."
msgstr ""

#. Tag: title
#. +> trunk5
#: editor-filters-distortionfx.docbook:21
#, no-c-format
msgid "Using the filter"
msgstr ""

#. Tag: para
#. +> trunk5
#: editor-filters-distortionfx.docbook:23
#, no-c-format
msgid "These are the distorting effects available:"
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:33
#, no-c-format
msgid "Type"
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:34
#, no-c-format
msgid "Preview"
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:41
#, no-c-format
msgid "<guilabel>Fish Eyes</guilabel>: warps the photograph around a 3D spherical shape to reproduce the common photograph 'Fish Eyes' effect."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:51
#, no-c-format
msgid "<guilabel>Twirl</guilabel>: spins the photograph to produce a Twirl pattern."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:61
#, no-c-format
msgid "<guilabel>Cylinder Horizontal</guilabel>: warps the photograph around a horizontal cylinder."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:71
#, no-c-format
msgid "<guilabel>Cylinder Vertical</guilabel>: warps the photograph around a vertical cylinder."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:81
#, no-c-format
msgid "<guilabel>Cylinder H/V</guilabel>: warps the photograph around a 2 cylinders, vertical and horizontal."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:91
#, no-c-format
msgid "<guilabel>Caricature</guilabel>: distorts photograph with 'Fish Eyes' effect inverted."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:101
#, no-c-format
msgid "<guilabel>Multiple Corners</guilabel>: splits the photograph like a multiple corners pattern."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:111
#, no-c-format
msgid "<guilabel>Waves Horizontal</guilabel>: distorts the photograph with horizontal waves."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:121
#, no-c-format
msgid "<guilabel>Waves Vertical</guilabel>: distorts the photograph with vertical waves."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:131
#, no-c-format
msgid "<guilabel>Block Waves 1</guilabel>: divides the image into cells and makes it look as if it is being viewed through glass blocks."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:141
#, no-c-format
msgid "<guilabel>Block Waves 2</guilabel>: like Block Waves 1 but with another version of glass blocks distortion."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:151
#, no-c-format
msgid "<guilabel>Circular Waves 1</guilabel>: distorts the photograph with circular waves."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:161
#, no-c-format
msgid "<guilabel>Circular Waves 2</guilabel>: other variation of Circular Waves effect."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:171
#, no-c-format
msgid "<guilabel>Polar Coordinates</guilabel>: converts the photograph from rectangular to polar coordinates."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:180
#, no-c-format
msgid "<guilabel>Unpolar Coordinates</guilabel>: Polar Coordinate effect inverted."
msgstr ""

#. Tag: entry
#. +> trunk5
#: editor-filters-distortionfx.docbook:190
#, no-c-format
msgid "<guilabel>Tiles</guilabel>: splits the photograph into square blocks and move them randomly inside the image."
msgstr ""

#. Tag: para
#. +> trunk5
#: editor-filters-distortionfx.docbook:205
#, no-c-format
msgid "Some effects can take a long time to run and cause high CPU load. You can always abort an effect by pressing <guilabel>Abort</guilabel> button during preview rendering."
msgstr ""

