#. +> trunk6
#: index.docbook:248
#, no-c-format
msgid "The interface also proposes buttons to edit pupils, add or remove them from groups."
msgstr ""

#. Tag: phrase
#. +> trunk6
#: index.docbook:255
#, no-c-format
msgid "pupil_add_to_group"
msgstr ""

#. Tag: phrase
#. +> trunk6
#: index.docbook:262
#, no-c-format
msgid "pupil_remove_from_group"
msgstr ""

#. Tag: para
#. +> trunk6
#: index.docbook:266
#, no-c-format
