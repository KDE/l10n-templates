msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:353
#, no-c-format
msgid "<action>Switch to editing mode</action> to edit the current hole."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: index.docbook:358
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Shift;<keycap>N</keycap></keycombo> </shortcut> <guimenu>Hole</guimenu> <guimenuitem>New</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:365
#, no-c-format
msgid "<action>Create a new hole.</action> (Only while editing.)"
msgstr ""

