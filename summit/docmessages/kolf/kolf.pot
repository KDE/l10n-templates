msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&End;</keycombo> </shortcut> <guimenu>Game</guimenu> <guimenuitem>End Game</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:304
#, no-c-format
msgid "<action>End the current game, without closing</action> &kolf;."
msgstr ""

