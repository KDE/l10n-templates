msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1247
#, no-c-format
msgid "Toggle the display of &kate;'s Filesystem Browser."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1257
#, no-c-format
msgid "From the top down, the Filesystem Browser consists of the following elements:"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: plugins.docbook:1262
#, no-c-format
msgid "A Toolbar"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1264
#, no-c-format
msgid "This contains standard navigations tool buttons:"
msgstr ""

#. Tag: guiicon
#. +> trunk6 stable6
#: plugins.docbook:1267
#, no-c-format
msgid "Back"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1268
#, no-c-format
msgid "Causes the folder view to <command>cd</command> to the previously displayed folder in the history. This button is disabled, if there is no previous item."
msgstr ""

#. Tag: guiicon
#. +> trunk6 stable6
#: plugins.docbook:1272
#, no-c-format
msgid "Forward"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1273
#, no-c-format
msgid "Causes the folder view to <command>cd</command> to the next folder in the history. This button is disabled, if there is no next folder."
msgstr ""

#. Tag: guiicon
#. +> trunk6 stable6
#: plugins.docbook:1282
#, no-c-format
msgid "Bookmarks"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1283
#, no-c-format
msgid "Opens a submenu to edit or add bookmarks and to add a new bookmark folder."
msgstr ""

#. Tag: guiicon
#. +> trunk6 stable6
#: plugins.docbook:1286
#, no-c-format
msgid "Current Document Folder"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:1287
