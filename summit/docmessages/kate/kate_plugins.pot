msgid "On a per project basis using the <filename>.kateproject</filename> file"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:4681
#, no-c-format
msgid "When reading the config, the plugin will first try to read the config from <filename>.kateproject</filename> file and then read the global config. Example:"
msgstr ""

#. Tag: screen
#. +> trunk6 stable6
#: plugins.docbook:4685
#, no-c-format
msgid ""
"{\n"
"    \"formatOnSave\": true,\n"
"    \"formatterForJson\": \"jq\",\n"
"    \"cmake-format\": {\n"
"        \"formatOnSave\": false\n"
"    },\n"
"    \"autopep8\": {\n"
"        \"formatOnSave\": false\n"
"    }\n"
"}"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:4687
#, no-c-format
msgid "The above:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: plugins.docbook:4689
