msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:868
#, no-c-format
msgid "This is a toggle item. Setting it on checked will make the Icon Border visible in the left side of the active editor, and vice versa. The Icon Border indicates the positions of the marked lines in the editor."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:875
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\"><keycap>F11</keycap></keycombo> </shortcut> <guimenu>View</guimenu> <guisubmenu>Borders</guisubmenu> <guimenuitem>Show Line Numbers</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:884
