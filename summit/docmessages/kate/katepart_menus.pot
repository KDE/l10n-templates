msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1194
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Highlighting</guisubmenu>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1199
#, no-c-format
msgid "Choose the Highlighting scheme you prefer for the active document. This overwrites the global highlighting mode set in <menuchoice> <guimenu>Settings</guimenu><guimenuitem>Configure Editor...</guimenuitem> </menuchoice> for your current document only."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1207
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Indentation</guisubmenu>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1212
