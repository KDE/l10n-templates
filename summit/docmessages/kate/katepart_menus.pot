msgid "<shortcut> <keycombo action=\"simul\"><keycap>F10</keycap></keycombo> </shortcut> <guimenu>View</guimenu> <guisubmenu>Word Wrap</guisubmenu> <guimenuitem>Dynamic Word Wrap</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:823
