msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:730
#, no-c-format
msgid "Adds another caret above the current cursor. The caret is placed in the line directly above and in the same column as the current cursor."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:735
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Alt;&Down;</keycombo> </shortcut> <guimenu>Selection</guimenu> <guimenuitem>Add Caret below Cursor</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:742
#, no-c-format
msgid "Adds another caret below the current cursor. The caret is placed in the line directly above and in the same column as the current cursor."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:747
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Alt;&Shift;<keycap>I</keycap></keycombo> </shortcut> <guimenu>Selection</guimenu> <guimenuitem>Add Cursors to Line Ends</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:754
#, no-c-format
