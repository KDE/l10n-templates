msgid "This submenu contains a list of all scripted actions. The list can easily be modified by <link linkend=\"dev-scripting-command-line\">writing your own scripts</link>. This way, &katepart; can be extended with user-defined tools."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1273
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Navigation</guisubmenu>"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1281
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Alt;&Shift;&Up;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Navigation</guisubmenu> <guimenuitem>Move cursor to previous matching indent</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1290
#, no-c-format
msgid "Moves the cursor to the first line above the current line that is indented at the same level as the current line."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1297
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Alt;&Shift;&Down;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Navigation</guisubmenu> <guimenuitem>Move cursor to next matching indent</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1306
#, no-c-format
msgid "Moves the cursor to the first line below the current line that is indented at the same level as the current line."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1322
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu>"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1330
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>Sort Selected Text</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1336
#, no-c-format
msgid "Sorts the selected text or whole document in ascending order."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1342
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Shift;&Down;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>Move Lines Down</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1351
#, no-c-format
msgid "Move selected lines down."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1357
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Shift;&Up;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>Move Lines Up</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1366
#, no-c-format
msgid "Move selected lines up."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1372
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Alt;&Down;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>Duplicate Selected Lines Down</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1381
#, no-c-format
msgid "Duplicates the selected lines down."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1387
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Alt;&Up;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>Duplicate Selected Lines Up</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1396
#, no-c-format
msgid "Duplicates the selected lines up."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1402
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>URI-encode selected text</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1408
#, no-c-format
msgid "Encodes the selected text so that it can be used as part of a query string in a &URL;, replacing the selection with the encoded text."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1415
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Editing</guisubmenu> <guimenuitem>URI-decode selected text</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1421
#, no-c-format
msgid "If part of the query string of a &URL; is selected, this will decode it and replace the selection with the original raw text."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1431
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu>"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1439
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Expand abbreviation</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1445
#, no-c-format
msgid "Converts the selected text to a pair of opening and closing &HTML; or &XML; tags. For example, if <userinput>div</userinput> is selected, this item will replace that with <computeroutput>&lt;div&gt;&lt;/div&gt;</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1453
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Wrap with tag</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1459
#, no-c-format
msgid "Wraps the selected text with the tag provided on the <link linkend=\"dev-scripting-command-line\">command line</link>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1466
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Move cursor to matching tag</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1472
#, no-c-format
msgid "If the cursor is inside an opening &HTML;/&XML; tag, this item will move it to the closing tag. If the cursor is inside the closing tag, it will instead move it to the opening tag."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1480
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Select &HTML;/&XML; tag contents inwards</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1486
#, no-c-format
msgid "When the cursor is inside a pair of &HTML;/&XML; tags, this option will change the selection to include the contents of those &HTML;/&XML; tags, without selecting the tags themselves."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1494
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Select &HTML;/&XML; tag contents outwards</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1500
#, no-c-format
msgid "When the cursor is inside a pair of &HTML;/&XML; tags, this item will change the selection to include the contents of those &HTML;/&XML; tags, including the tags themselves."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1508
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Toggle Comment</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1514
#, no-c-format
msgid "If the selected portion is not a comment, this item will enclose that portion in &HTML;/&XML; comments (&eg; <computeroutput>&lt;!-- selected text --&gt;</computeroutput>). If the selected portion is a comment, the comment tags will be removed instead."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1525
#, no-c-format
msgid "<guimenu>Tools</guimenu><guisubmenu>Scripts</guisubmenu><guisubmenu>Emmet</guisubmenu> <guimenuitem>Delete tag under cursor</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1529
#, no-c-format
msgid "If the cursor is presently inside a &HTML;/&XML; tag, this item will delete the entire tag."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1536
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Decrement number by 1</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1542
#, no-c-format
msgid "This item will subtract one from the currently selected text, if it is a number. For example, if <userinput>5</userinput> is selected, it will become <computeroutput>4</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1550
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Decrement number by 10</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1556
#, no-c-format
msgid "This item will subtract 10 from the currently selected text, if it is a number. For example, if <userinput>15</userinput> is selected, it will become <computeroutput>5</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1564
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Decrement number by 0.1</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1570
#, no-c-format
msgid "This item will subtract 0.1 from the currently selected text, if it is a number. For example, if <userinput>4.5</userinput> is selected, it will become <computeroutput>4.4</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1578
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Increment number by 1</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1584
#, no-c-format
msgid "This item will add one to the currently selected text, if it is a number. For example, if <userinput>5</userinput> is selected, it will become <computeroutput>6</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1592
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Increment number by 10</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1598
#, no-c-format
msgid "This item will add 10 to the currently selected text, if it is a number. For example, if <userinput>5</userinput> is selected, it will become <computeroutput>15</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1606
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Scripts</guisubmenu> <guisubmenu>Emmet</guisubmenu> <guimenuitem>Increment number by 0.1</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1612
#, no-c-format
msgid "This item will add 0.1 to the currently selected text, if it is a number. For example, if <userinput>4.5</userinput> is selected, it will become <computeroutput>4.6</computeroutput>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1628
#, no-c-format
msgid "<shortcut> <keycap>F7</keycap> </shortcut> <guimenu>Tools</guimenu> <guimenuitem>Switch to Command Line</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1636
#, no-c-format
msgid "Displays the &katepart; command line at the bottom of the window. In the command line, type <userinput>help</userinput> to get help and <userinput>help list</userinput> to get a list of commands. For more information on the command line, see <link linkend=\"advanced-editing-tools-commandline\">The Editor Component Command Line</link>."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1645
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Space;</keycombo> </shortcut> <guimenu>Tools</guimenu> <guimenuitem>Invoke Code Completion</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1653
#, no-c-format
msgid "Manually invoke command completion, usually by using a shortcut bound to this action."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1658
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guimenuitem>Word Completion</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1663
#, no-c-format
msgid "<guimenuitem>Reuse Word Below</guimenuitem> (<keycombo action=\"simul\">&Ctrl; <keycap>9</keycap></keycombo>) and <guimenuitem>Reuse Word Above</guimenuitem> (<keycombo action=\"simul\">&Ctrl; <keycap>8</keycap></keycombo>) complete the currently typed text by searching for similar words backward or forward from the current cursor position. <guimenuitem>Shell Completion</guimenuitem> pops up a completion box with matching entries."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1673
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Shift;<keycap>O</keycap></keycombo> </shortcut> <guimenu>Tools</guimenu> <guisubmenu>Spelling</guisubmenu> <guimenuitem>Automatic Spell Checking</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1682
#, no-c-format
msgid "When <guimenuitem>Automatic Spell Checking</guimenuitem> is enabled, wrongly spelled text is underlined in the document on-the-fly."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1688
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Spelling</guisubmenu> <guimenuitem>Spelling...</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1699
#, no-c-format
msgid "<action>This initiates the spellchecking program - a program designed to help the user catch and correct any spelling errors.</action> Clicking on this entry will start the checker and bring up the speller dialog box through which the user can control the process. There are four settings lined up vertically in the center of the dialog with their corresponding labels just to the left. Starting at the top they are:"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: menus.docbook:1709
#, no-c-format
msgid "Unknown word:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1710
#, no-c-format
msgid "Here, the spellchecker indicates the word currently under consideration. This happens when the checker encounters a word not in its dictionary - a file containing a list of correctly spelled words against which it compares each word in the editor."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: menus.docbook:1717
#, no-c-format
msgid "Replace with:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1718
#, no-c-format
msgid "If the checker has any similar words in its dictionary the first one will be listed here. The user can accept the suggestion, type in his or her own correction, or choose a different suggestion from the next box."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: menus.docbook:1724
#, no-c-format
msgid "Language:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1725
#, no-c-format
msgid "If you have installed multiple dictionaries, here you can select which dictionary/language should be used."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1730
#, no-c-format
msgid "On the right side of the dialog box are 6 buttons that allow the user to control the spellcheck process. They are:"
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1736
#, no-c-format
msgid "Add to Dictionary"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1737
#, no-c-format
msgid "Pressing this button adds the <guilabel>Unknown word </guilabel> to the checker's dictionary. This means that in the future the checker will always consider this word to be correctly spelled."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: menus.docbook:1744
#, no-c-format
msgid "Suggest"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1745
#, no-c-format
msgid "The checker may list here a number of possible replacements for the word under consideration. Clicking on any one of the suggestions will cause that word to be entered in the <guilabel>Replace with</guilabel> box, above."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1752
#, no-c-format
msgid "Replace"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1753
#, no-c-format
msgid "This button has the checker replace the word under consideration in the document with the word in the <guilabel>Replace with</guilabel> box."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1759
#, no-c-format
msgid "Replace All"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1760
#, no-c-format
msgid "This button causes the checker to replace not only the current <guilabel>Unknown word:</guilabel> but to automatically make the same substitution for any other occurrences of this <guilabel>Unknown word</guilabel> in the document."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1767
#, no-c-format
msgid "Ignore"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1768
#, no-c-format
msgid "Activating this button will have the checker move on without making any changes."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1773
#, no-c-format
msgid "Ignore All"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1774
#, no-c-format
msgid "This button tells the checker to do nothing with the current <guilabel>Unknown word:</guilabel> and to pass over any other instances of the same word."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1776
#, no-c-format
msgid "This only applies to the current spellcheck run. If the checker is run again later it will stop on this same word."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1783
#, no-c-format
msgid "Three more buttons are located horizontally along the bottom of the spellcheck dialog. They are:"
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1789
#, no-c-format
msgid "Help"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1790
#, no-c-format
msgid "This invokes the &kde; help system with the help page for this dialog."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1795
#, no-c-format
msgid "Finished"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1796
#, no-c-format
msgid "This button ends the spellcheck process, and returns to the document."
msgstr ""

#. Tag: guibutton
#. +> trunk6 stable6
#: menus.docbook:1801
#, no-c-format
msgid "Cancel"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1802
#, no-c-format
msgid "This button cancels the spellcheck process, all modifications are reverted, and you will return to your document."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1813
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Spelling</guisubmenu> <guimenuitem>Spelling (from cursor)...</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1819
#, no-c-format
msgid "This initiates the spellchecking program but it starts where your cursor is instead of at the beginning of the document."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1825
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Spelling</guisubmenu> <guimenuitem>Spellcheck Selection...</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1831
#, no-c-format
msgid "Spellchecks the current selection."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1836
#, no-c-format
msgid "<guimenu>Tools</guimenu> <guisubmenu>Spelling</guisubmenu> <guimenuitem>Change Dictionary</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1842
#, no-c-format
msgid "Displays a drop down box with all available dictionaries for spellchecking at the bottom of the editor window. This allows easy switching of the spellcheck dictionary &eg; for automatic spellcheck of text in different languages."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: menus.docbook:1852
#, no-c-format
msgid "The Settings and Help Menu"
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:1856
#, no-c-format
msgid "<guimenu>Settings</guimenu> <guisubmenu>Editor Color Theme</guisubmenu>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1860
#, no-c-format
msgid "This menu lists the available color schemes. You can change the schema for the current view here, to change the default schema you need to use the <ulink url=\"help:/katepart/config-dialog-editor.html#prefcolors\">Fonts &amp; Colors</ulink> page of the config dialog."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:1868
#, no-c-format
msgid "&katepart; has the common &kde; <guimenu>Settings</guimenu> and <guimenu>Help</guimenu> menu items, for more information read the sections about the <ulink url=\"help:/fundamentals/menus.html#menus-settings\">Settings Menu</ulink> and <ulink url=\"help:/fundamentals/menus.html#menus-help\">Help Menu</ulink> of the &kde; Fundamentals."
msgstr ""
