msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:759
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Alt;<keycap>J</keycap></keycombo> </shortcut> <guimenu>Selection</guimenu> <guimenuitem>Find and Select Next Occurrence</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:766
#, no-c-format
msgid "Finds the next occurrence of the word currently under the cursor, selects it and adds a cursor."
msgstr ""

#. Tag: menuchoice
#. +> trunk6 stable6
#: menus.docbook:771
#, no-c-format
msgid "<shortcut> <keycombo action=\"simul\">&Ctrl;&Alt;&Shift;<keycap>J</keycap></keycombo> </shortcut> <guimenu>Selection</guimenu> <guimenuitem>Find and Select All Occurrences</guimenuitem>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: menus.docbook:778
#, no-c-format
msgid "Finds all occurrences of the word currently under the cursor, selects them and adds a cursor for each of them."
msgstr ""

