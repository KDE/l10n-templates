#, no-c-format
msgid "OPTIONS"
msgstr ""

#. Tag: option
#. +> trunk6 stable5
#: man-symmy.1.docbook:112
#, no-c-format
msgid "-e, --encrypt"
msgstr ""

#. Tag: para
#. +> trunk6 stable5
#: man-symmy.1.docbook:115
#, no-c-format
msgid "Encrypts the given list of files after asking a passphrase. This option is implicitly set if there are no other options."
msgstr ""

#. Tag: option
#. +> trunk6 stable5
#: man-symmy.1.docbook:123
#, no-c-format
msgid "-d, --decrypt"
msgstr ""

#. Tag: para
#. +> trunk6 stable5
#: man-symmy.1.docbook:126
#, no-c-format
msgid "Decrypts the given list of files after asking the decryption key. Note that <option>encrypt</option> and <option>decrypt</option> are mutually exclusive."
msgstr ""

