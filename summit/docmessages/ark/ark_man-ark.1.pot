#. Tag: title
#. +> trunk6 stable6
#: man-ark.1.docbook:60
#, no-c-format
msgid "Description"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:61
#, no-c-format
msgid "&ark; is a program for managing various compressed file formats within &kde;. Archives can be viewed, extracted, created and modified with &ark;. The program can handle various formats such as <application>tar</application>, <application>gzip</application>, <application>bzip2</application>, <application>zip</application>, <application>rar</application> (when the appropriate libraries or command-line programs are installed)."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: man-ark.1.docbook:72
#, no-c-format
msgid "Operation modes"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:73
#, no-c-format
msgid "&ark; can be used either as a stand-alone &GUI; program as well as a command-line program in order to perform some specific tasks."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:75
#, no-c-format
msgid "If invoked without the -b (--batch) or -c (--add) options, &ark; is started as a normal &GUI; program."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:77
#, no-c-format
msgid "When the -b (--batch) option is used, &ark; can be used to extract the contents of one or more files directly from the command-line, without launching its &GUI;."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:80
#, no-c-format
msgid "When the -c (--add) option is used, &ark; prompts for files that should be added to a new archive or to an existing archive."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: man-ark.1.docbook:87
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:91
#, no-c-format
msgid "-d, --dialog"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:93
#, no-c-format
msgid "Show a dialog for specifying the options for a batch or add operation."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:97
#, no-c-format
msgid "-m, --mimetypes"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:99
#, no-c-format
msgid "List supported &MIME; types."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:103
#, no-c-format
msgid "-o, --destination <replaceable>directory</replaceable>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:105
#, no-c-format
msgid "Default the extraction directory to <replaceable>directory</replaceable>. If not passed, the current path is used."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: man-ark.1.docbook:112
#, no-c-format
msgid "Options for adding files"
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:115
#, no-c-format
msgid "-c, --add"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:117
#, no-c-format
msgid "Query the user for an archive filename and add specified files to it. Quit when finished."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:122
#, no-c-format
msgid "-t, --add-to <replaceable>filename</replaceable>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:124
#, no-c-format
msgid "Add the specified files to <replaceable>filename</replaceable>. Create archive if it does not exist. Quit when finished."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:129
#, no-c-format
msgid "-p, --changetofirstpath"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:131
#, no-c-format
msgid "Change the current directory to the first entry and add all other entries relative to this one."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:137
#, no-c-format
msgid "-f, --autofilename <replaceable>suffix</replaceable>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:139
#, no-c-format
msgid "Automatically choose a filename, with the selected <replaceable>suffix</replaceable> (for example rar, tar.gz, zip or any other supported types)."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: man-ark.1.docbook:147
#, no-c-format
msgid "Options for batch extraction"
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:150
#, no-c-format
msgid "-b, --batch"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:152
#, no-c-format
msgid "Use the batch interface instead of the usual dialog. This option is implied if more than one url is specified."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:158
#, no-c-format
msgid "-e, --autodestination"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:160
#, no-c-format
msgid "The destination argument will be set to the path of the first file supplied."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:166
#, no-c-format
msgid "-a, --autosubfolder"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:168
#, no-c-format
msgid "Archive contents will be read, and if detected to not be a single folder archive, a subfolder by the name of the archive will be created."
msgstr ""

#. Tag: option
#. +> trunk6 stable6
#: man-ark.1.docbook:174
#, no-c-format
msgid "-O, --opendestination"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: man-ark.1.docbook:176
#, no-c-format
msgid "Open the destination folder when the extraction is complete."
msgstr ""

