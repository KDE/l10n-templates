msgstr ""

#. Tag: option
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:83
#, no-c-format
msgid "--global"
msgstr ""

#. Tag: para
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:85
#, no-c-format
msgid "Ignores any user-set files (in <varname>XDG_DATA_HOME</varname>). This is currently only supported on &UNIX; systems."
msgstr ""

#. Tag: option
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:90
#, no-c-format
msgid "--noincremental"
msgstr ""

#. Tag: option
#. +> trunk5
#: man-kbuildsycoca5.8.docbook:97
#, no-c-format
msgid "--nosignal"
msgstr ""

#. Tag: para
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:92
#, no-c-format
msgid "Rather than using the existing cache and only updating the information that has changed or been added, start with an empty cache. Ignored if <option>--global</option> is set."
msgstr ""

#. Tag: option
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:97
#, no-c-format
msgid "--menutest"
msgstr ""

#. Tag: para
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:99
#, no-c-format
msgid "Test the generation of the application menu database. Does not actually build the cache."
msgstr ""

#. Tag: para
#. +> trunk5
#: man-kbuildsycoca5.8.docbook:99
#, no-c-format
msgid "Ordinarily, <command>kbuildsycoca5</command> sends a signal over &DBus; once the cache is updated to all the applications that use KService so that they are always working with the latest information. This option suppresses that behaviour."
msgstr ""

#. Tag: option
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:104
#, no-c-format
msgid "--testmode"
msgstr ""

#. Tag: para
#. +> trunk6 trunk5
#: man-kbuildsycoca6.8.docbook:106
#, no-c-format
msgid "Use the QStandardPaths \"test mode\" to avoid interfering with user data. This is intended for use with unit tests."
msgstr ""

