msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:30
#, no-c-format
msgid "You can use the gdrive KIO Worker like this:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:31
#, no-c-format
msgid "<userinput>gdrive://</userinput> or <userinput>gdrive://<replaceable>foo@example.com</replaceable></userinput>."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:33
#, no-c-format
msgid "The first URL shows an OAuth login dialog from Google, if no Drive account has been authenticated yet, or the list of your authenticated accounts instead."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:36
#, no-c-format
msgid "The <userinput>gdrive://</userinput> URL can also be used at any time to add an additional account."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:38
#, no-c-format
msgid "The URL <userinput>gdrive://<replaceable>foo@example.com</replaceable></userinput> lists all the files and folders from the top level of the <userinput>foo@example.com</userinput> Drive account."
msgstr ""
