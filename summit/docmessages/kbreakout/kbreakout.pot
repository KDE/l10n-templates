msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:290
#, no-c-format
msgid "Gives the player an additional life."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:302
#, no-c-format
msgid "<phrase>Lose Life Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:306
#, no-c-format
msgid "<term>Lose Life Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:310
#, no-c-format
msgid "Makes the player lose all balls and resets the bar."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:322
#, no-c-format
msgid "<phrase>Enlarge Bar Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:326
#, no-c-format
msgid "<term>Enlarge Bar Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:330
#, no-c-format
msgid "Enlarges the bar."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:342
#, no-c-format
msgid "<phrase>Shrink Bar Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:346
#, no-c-format
msgid "<term>Shrink Bar Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:350
#, no-c-format
msgid "Shrinks the bar."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:362
#, no-c-format
msgid "<phrase>Split Balls Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:366
#, no-c-format
msgid "<term>Split Balls Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:370
#, no-c-format
msgid "Doubles the balls."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:382
#, no-c-format
msgid "<phrase>Add Balls Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:386
#, no-c-format
msgid "<term>Add Balls Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:390
#, no-c-format
msgid "Gives the player an extra ball attached to the bar, which can be fired whenever the player wants."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:402
#, no-c-format
msgid "<phrase>Sticky Bar Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:406
#, no-c-format
msgid "<term>Sticky Bar Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:410
#, no-c-format
msgid "After catching this gift, every time a ball touches the bar it remains attached to it, allowing the player to fire again."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:422
#, no-c-format
msgid "<phrase>More Exploding Bricks Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:426
#, no-c-format
msgid "<term>More Exploding Bricks Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:430
#, no-c-format
msgid "Makes all bricks to the top, bottom, right, and left of exploding bricks exploding bricks themselves. This also applies to hidden and unbreakable bricks."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:442
#, no-c-format
msgid "<phrase>Burning Ball Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:446
#, no-c-format
msgid "<term>Burning Ball Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:450
#, no-c-format
msgid "Transforms all the balls into burning balls. When a burning ball hits a brick also the bricks to the top, bottom, left and right of it get destroyed. A burning ball destroys all types of bricks regardless of their types."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:462
#, no-c-format
msgid "<phrase>Unstoppable Ball Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:466
#, no-c-format
msgid "<term>Unstoppable Ball Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:470
#, no-c-format
msgid "Transforms all the balls into unstoppable balls. An unstoppable ball destroys all bricks it encounters on its path, regardless of their type, and never bounces against them."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:482
#, no-c-format
msgid "<phrase>Magic Eye Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:486
#, no-c-format
msgid "<term>Magic Eye Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:490
#, no-c-format
msgid "Shows all hidden bricks."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:502
#, no-c-format
msgid "<phrase>Magic Wand Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:506
#, no-c-format
msgid "<term id=\"gift-magic-wand\">Magic Wand Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:510
#, no-c-format
msgid "Transforms all multiple and unbreakable bricks into simple bricks."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:522
#, no-c-format
msgid "<phrase>Increase Speed Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:526
#, no-c-format
msgid "<term>Increase Speed Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:530
#, no-c-format
msgid "Increases the speed of the ball by 30% of the current speed."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:542
#, no-c-format
msgid "<phrase>Decrease Speed Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:546
#, no-c-format
msgid "<term>Decrease Speed Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:550
#, no-c-format
msgid "Decreases the speed of the ball dividing the current speed by 1.3."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:562
#, no-c-format
msgid "<phrase>Next Level Gift</phrase>"
msgstr ""

#. Tag: term
#. +> trunk6 stable6
#: index.docbook:566
#, no-c-format
msgid "<term>Next Level Gift</term>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:570
#, no-c-format
msgid "Opens the next level of the game immediately."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:579
#, no-c-format
msgid "Strategies and Tips"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:581
#, no-c-format
msgid "If possible, it is usually a good idea to send the ball above the bricks, doing so you will have a smaller chance of losing the ball. Sometimes you need to first make a small hole in the layer of brick to accomplish this. The smaller the hole, the smaller the chance the balls comes back down."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:582
#, no-c-format
msgid "When catching the add ball gift it is wise to not fire it right away but to keep in instead, in case the other balls are lost. It is also advisable to not catch a sticky bar gift in such a case, as doing so would force you to fire all balls."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:583
#, no-c-format
msgid "When receiving a next level gift you may not want to catch it if you know that there are add life gifts in the level."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:584
#, no-c-format
msgid "In levels where there are a lot of hidden bricks there may be a way to win the level without having to destroy most of them."
msgstr ""

