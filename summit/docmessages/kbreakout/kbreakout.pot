#. Tag: term
#. +> trunk6 stable6
#: index.docbook:266
#, no-c-format
msgid "Score Gifts"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:270
#, no-c-format
msgid "If caught they award the player the number of points written on the gift."
msgstr ""

#. Tag: phrase
#. +> trunk6 stable6
#: index.docbook:282
#, no-c-format
msgid "<phrase>Add Life Gift</phrase>"
