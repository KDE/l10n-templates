#. Tag: userinput
#. +> trunk6 stable6
#: index.docbook:2705
#, no-c-format
msgid "<command>/exec </command> <replaceable>tinyurl https://konversation.kde.org</replaceable>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2706
#, no-c-format
msgid "Sample output: <replaceable>https://tinyurl.com/y4rk8wa3</replaceable>"
msgstr ""

#. Tag: userinput
#. +> trunk6 stable6
#: index.docbook:2711 index.docbook:2716
#, no-c-format
msgid "<command>/exec </command> <replaceable>uptime</replaceable>"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2714
#, no-c-format
msgid "This script outputs the result of the command \"uptime\" and formats it appropriately."
