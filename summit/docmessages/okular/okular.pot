#. Tag: title
#. +> trunk6 stable6
#: index.docbook:2525
#, no-c-format
msgid "<title>General</title>"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2528
#, no-c-format
msgid "Use smooth scrolling"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2530
#, no-c-format
msgid "Whether to display animated transitions when scrolling using the mouse wheel and keyboard."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2534
#, no-c-format
msgid "Show scrollbars"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2536
#, no-c-format
msgid "Whether to show scrollbars for the document view."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2540
#, no-c-format
msgid "Link the thumbnails with the page"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2542
#, no-c-format
msgid "Whether the thumbnails view should always display the current page or not."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2547
#, no-c-format
msgid "Show hints and info messages"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2549
#, no-c-format
msgid "Whether to show some informative messages on startup, file load, &etc;"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2554
#, no-c-format
msgid "Notify about embedded files, forms, or signatures"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2556
#, no-c-format
msgid "Whether to show informative panels about embedded files, forms or signatures &etc;"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2561
#, no-c-format
msgid "Display document title in titlebar if available"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2563
#, no-c-format
msgid "Whether to show the current document title in the titlebar of &okular; window. If no metadata for title found in the document or this item is unchecked &okular; shows filename of the document."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2569
#, no-c-format
msgid "When not displaying document title"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2571
#, no-c-format
msgid "You can choose any of two options, <guilabel>Display file name only</guilabel> or <guilabel>Display full file path</guilabel>."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2576
#, no-c-format
msgid "Use custom background color"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2578
#, no-c-format
msgid "Enables <ulink url=\"help:/fundamentals/colors.html\">choosing the document background color</ulink> (the color around the displayed page). By default, the &Qt; toolkit color is used when this option is unchecked."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2583
#, no-c-format
msgid "Open new files in tabs"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2585
#, no-c-format
msgid "Whether to open new documents in tabs. The tabs are disabled by default."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2586
#, no-c-format
msgid "The default shortcuts to switch between tabs are <keycombo>&Ctrl;<keycap>.</keycap></keycombo> (<guilabel>Next tab</guilabel>) and <keycombo>&Ctrl;<keycap>,</keycap></keycombo> (<guilabel>Previous tab</guilabel>)."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2592
#, no-c-format
msgid "Switch to existing tab if file is already open"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2594
#, no-c-format
msgid "Whether &okular; should switch to a tab if the file is already open instead of opening a new tab. This option is enabled by default. Can be toggled only if <guilabel>Open new files in tabs</guilabel> is enabled."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2598
#, no-c-format
msgid "Obey DRM limitations"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2600
#, no-c-format
msgid "Whether &okular; should obey <firstterm>DRM</firstterm> (Digital Rights Management) restrictions. DRM limitations are used to make it impossible to perform certain actions with &PDF; documents, such as copying content to the clipboard. Note that in some configurations of &okular;, this option is not available."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2608
#, no-c-format
msgid "Reload document on file change"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2610
#, no-c-format
msgid "Whether opened files should be automatically checked for changes and updated, if necessary."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2615
#, no-c-format
msgid "Show backend selection dialog"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2617
#, no-c-format
msgid "Whether &okular; should ask the user which backend to use in case of more than one backend able to open the current file. If unchecked, &okular; will use the backend with the highest priority."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2623
#, no-c-format
msgid "Right to left reading direction"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2625
#, no-c-format
msgid "Whether to use right to left reading direction by default for the opened files. Can be useful for <ulink url=\"https://en.wikipedia.org/wiki/Right-to-left\">some writing systems</ulink>."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2631
#, no-c-format
msgid "Open in continuous mode by default"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2633
#, no-c-format
msgid "Whether to use the continuous mode by default. In continuous mode, all pages of the document are shown, and you can smoothly scroll through them. Otherwise, only one page (or one row of pages) is shown at a time."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2639
#, no-c-format
msgid "When using Browse Tool, wrap cursor at screen edges"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2641
#, no-c-format
msgid "Allows endless scrolling around the entire document with the <link linkend=\"menutools\">Browse Tool</link>. If enabled, the mouse cursor will wrap to the opposite end of the screen when it touches the borders while dragging the document. This feature is only supported on X11."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2647
#, no-c-format
msgid "Overview columns"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2649
#, no-c-format
msgid "This option represents the number of columns to use in the overview mode."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2655
#, no-c-format
msgid "Page Up/Down overlap"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2657
#, no-c-format
msgid "Here you can define the percentage of the current viewing area that should be visible after pressing &PgUp;/&PgDn; keys."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2663
#, no-c-format
msgid "Default Zoom"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2665
#, no-c-format
msgid "This option specifies the default zoom mode for files which were never opened before. For those files which were opened before, the previous zoom mode is applied."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:2675
#, no-c-format
msgid "<title>Accessibility</title>"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2678
#, no-c-format
msgid "Draw border around links"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2680
#, no-c-format
msgid "Whether to draw a border around links."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2684
#, no-c-format
msgid "Change colors"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2686
#, no-c-format
msgid "<action>Enables</action> the color changing options."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2690
#, no-c-format
msgid "Color mode"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2692
#, no-c-format
msgid "<action>Defines</action> the color changing mode."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2695
#, no-c-format
msgid "Invert Colors"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2697
#, no-c-format
msgid "<action>Inverts</action> colors on the view, &ie; black objects will be shown white."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2701
#, no-c-format
msgid "Change Paper Color"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2703
#, no-c-format
msgid "<action>Changes</action> the paper's color, &ie; the document's background."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2704
#, no-c-format
msgid "The color can be selected using the <guilabel>Paper color</guilabel> swatch below."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2709
#, no-c-format
msgid "Change Dark &amp; Light Colors"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2711
#, no-c-format
msgid "<action>Changes</action> the dark and light color to your preference, that means black will not be rendered as black but as the selected dark color and white will not be rendered as white but as the selected light color."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2717
#, no-c-format
msgid "Convert to Black &amp; White"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2719
#, no-c-format
msgid "<action>Converts</action> the document to black and white. You can set the threshold and the contrast. Setting the threshold to a higher value by moving it to the right will result in lighter grays used."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2725
#, no-c-format
msgid "Invert Lightness"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2727
#, no-c-format
msgid "<action>Inverts</action> lightness of all colors. Light and dark colors will be swapped, but hue and saturation will not be affected. The Contrast in images will usually be worse than in <guilabel>Invert Luma (sRGB Linear)</guilabel>."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2733
#, no-c-format
msgid "Invert Luma (sRGB Linear)"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2735
#, no-c-format
msgid "<action>Inverts</action> luma of all colors. Light and dark will be swapped, but hue and saturation will not be affected. The Contrast in images is preserved better than in <guilabel>Invert Lightness</guilabel>, but graphics and colorful text markup usually look worse. Uses sRGB luma coefficients, but no gamma correction."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2743
#, no-c-format
msgid "Invert Luma (Symmetric)"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2745
#, no-c-format
msgid "<action>Inverts</action> luma of all colors, using symmetric luma coefficients. Light and dark will be swapped, but hue and saturation will not be affected. Very similar to <guilabel>Invert Lightness</guilabel>, but the contrast is in some cases better."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2752
#, no-c-format
msgid "Shift Hue Positive"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2754
#, no-c-format
msgid "<action>Shifts</action> hue of all colors by 120 degrees. Can mitigate some contrast problems in colorful graphics."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2759
#, no-c-format
msgid "Shift Hue Negative"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2761
#, no-c-format
msgid "<action>Shifts</action> hue of all colors by 240 degrees. Can mitigate some contrast problems in colorful graphics."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2766
#, no-c-format
msgid "You can configure keyboard shortcuts and toolbar actions for the color modes using the corresponding menu items in the <guimenu>Settings</guimenu> menu of the &okular; main window."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2770
#, no-c-format
msgid "Text-to-speech engine"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2772
#, no-c-format
msgid "<action>Allows you to choose</action> the text-to-speech (<abbrev>TTS</abbrev>) engine to be used by &okular;. The default is <application>speechd</application> <abbrev>TTS</abbrev>."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2777
#, no-c-format
msgid "Text-to-speech voice"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2779
#, no-c-format
msgid "<action>Allows you to choose</action> a voice for the text-to-speech engine to be used by &okular;. You can use all languages and voices depending on the available synthesizers on your platform. On &Linux;, speech-dispatcher is used by default to determine available voices."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:2787
#, no-c-format
msgid "<title>Performance</title>"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2790
#, no-c-format
msgid "Enable transparency effects"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2792
#, no-c-format
msgid "Draw selections and other special graphics using transparency effects. Disable the option to draw them using outline or opaque fill styles and increase speed on selections."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2798
#, no-c-format
msgid "Memory usage"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2800
#, no-c-format
msgid "&okular; can achieve best performance by tuning the memory usage, based on your system and your tastes. The more memory you let it to use, the faster the program will behave. The Default profile is good for every system, but you can prevent &okular; from using more memory than necessary by selecting the Low profile, or let it get the most out of your system using Aggressive. Use Greedy profile to preload all pages without risk of system memory overfull (only 50% of total memory or free memory will be used)."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2808
#, no-c-format
msgid "Rendering options"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2810
#, no-c-format
msgid "Using this group of options you can improve text and image rendering in &okular;. The result depends on the device to display the document. <guilabel>Enable Text Antialias</guilabel> and <guilabel>Enable Graphics Antialias</guilabel> items can be used to switch on and off <ulink url=\"https://en.wikipedia.org/wiki/Spatial_anti-aliasing\">spatial anti-aliasing</ulink> of text and images in document, correspondingly. <guilabel>Enable Text Hinting</guilabel> item is meant to be a switcher for <ulink url=\"https://en.wikipedia.org/wiki/Font_hinting\">font hinting</ulink>. Antialiasing and hinting change how the documents are displayed, you may want to tweak them to your preference."
msgstr ""

#. Tag: title
#. +> trunk6 stable6
#: index.docbook:2819
#, no-c-format
msgid "<title>Presentation</title>"
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2822
#, no-c-format
msgid "Advance every"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2824
#, no-c-format
msgid "Enables automatic advancing of pages given a time period."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2828
#, no-c-format
msgid "Loop after last page"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2830
#, no-c-format
msgid "When navigating on presentation mode and going past the last page the first page will appear."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2834
#, no-c-format
msgid "Touch navigation"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2836
#, no-c-format
msgid "Allows you to fine-tune touchscreen navigation with tapping in the presentation using the following three possible values:"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2839
#, no-c-format
msgid "<guilabel>Tap left/right side to go back/forward</guilabel>: Tapping on the left (right) half of the screen makes the presentation go to the previous (next) slide."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2842
#, no-c-format
msgid "<guilabel>Tap anywhere to go forward</guilabel> (default): Tapping on the screen makes the presentation go to the next slide, no matter where the screen is being tapped."
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2845
#, no-c-format
msgid "<guilabel>Disabled</guilabel>: Screen tapping does not do anything."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2851
#, no-c-format
msgid "Background color"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2853
#, no-c-format
msgid "The color that will fill the part of the screen not covered by the page when on presentation mode."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2857
#, no-c-format
msgid "Mouse cursor"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2859
#, no-c-format
msgid "Whether the mouse should be always hidden, always shown or hidden after a small time of inactivity."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2863
#, no-c-format
msgid "Show progress indicator"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2865
#, no-c-format
msgid "Whether to show a progress circle that shows the current page and the total number of pages on the upper right corner of the presentation screen every time you change the page."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2870
#, no-c-format
msgid "Show summary page"
msgstr ""

#. Tag: para
#. +> trunk6 stable6
#: index.docbook:2872
#, no-c-format
msgid "Whether to show a summary page at the beginning of the presentation with the title, author and number of pages of the document."
msgstr ""

#. Tag: guilabel
#. +> trunk6 stable6
#: index.docbook:2876
#, no-c-format
msgid "Enable transitions"
msgstr ""

#. Tag: para
