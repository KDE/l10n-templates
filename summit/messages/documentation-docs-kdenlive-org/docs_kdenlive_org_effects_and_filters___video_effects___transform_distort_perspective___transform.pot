#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:98
msgid "Multiply"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:99
msgid "The source color is multiplied by the destination color and replaces the destination. The resultant color is always at least as dark as either the source or destination color. Multiplying any color with black results in black. Multiplying any color with white preserves the original color."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:100
msgid "Screen"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:101
msgid "Multiplies the complements of the backdrop and source color values, then complements the result. The result color is always at least as light as either of the two constituent colors. Screening any color with white produces white; screening with black leaves the original color unchanged. The effect is similar to projecting multiple photographic slides simultaneously onto a single screen."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:102
msgid "Overlay"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:103
msgid "Multiplies or screens the colors, depending on the backdrop color value. Source colors overlay the backdrop while preserving its highlights and shadows. The backdrop color is not replaced but is mixed with the source color to reflect the lightness or darkness of the backdrop."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:104
msgid "Darken"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:105
msgid "Selects the darker of the backdrop and source colors. The backdrop is replaced with the source where the source is darker; otherwise, it is left unchanged."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:106
msgid "Lighten"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:107
msgid "Selects the lighter of the backdrop and source colors. The backdrop is replaced with the source where the source is lighter; otherwise, it is left unchanged."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:108
msgid "Color dodge"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:109
msgid "Brightens the backdrop color to reflect the source color. Painting with black produces no changes."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:110
msgid "Color burn"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:111
msgid "Darkens the backdrop color to reflect the source color. Painting with white produces no change."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:112
msgid "Hard light"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:113
msgid "Multiplies or screens the colors, depending on the source color value. The effect is similar to shining a harsh spotlight on the backdrop."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:114
msgid "Soft light"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:115
msgid "Darkens or lightens the colors, depending on the source color value. The effect is similar to shining a diffused spotlight on the backdrop."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:116
msgid "Difference"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:117
msgid "Subtracts the darker of the two constituent colors from the lighter color. Painting with white inverts the backdrop color; painting with black produces no change."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:118
msgid "Exclusion"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/transform_distort_perspective/transform.rst:119
msgid "Produces an effect similar to that of the Difference mode but lower in contrast. Painting with white inverts the backdrop color; painting with black produces no change."
msgstr ""

