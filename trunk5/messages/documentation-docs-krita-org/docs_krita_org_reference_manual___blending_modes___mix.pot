#: ../../reference_manual/blending_modes/mix.rst:184
msgid "Greater (Blending Mode)"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:188
msgid "Greater"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:192
msgid "This blending mode is called \"Greater\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:194
msgid "A blending mode which checks whether the painted color is painted with a higher opacity than the existing colors. If so, it paints over them, if not, it doesn't paint at all."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:196
msgid "A similar mode is `Marker`_, which has the same behavior in regards to opacity, but will always mix the colors."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:201
#: ../../reference_manual/blending_modes/mix.rst:205
msgid "Hard Mix"
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:209
msgid "This blending mode is called \"Hard Mix\" in English."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:211
msgid "Similar to Overlay."
msgstr ""

#: ../../reference_manual/blending_modes/mix.rst:213
msgid "Mixes both Color Dodge and Burn blending modes. If the color of the upper layer is darker than 50%, the blending mode will be Burn, if not the blending mode will be Color Dodge."
msgstr ""

