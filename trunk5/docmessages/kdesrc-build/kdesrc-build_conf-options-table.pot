#: conf-options-table.docbook:55
#, no-c-format
msgid "colorful-output"
msgstr ""

#. Tag: para
#: conf-options-table.docbook:59
#, no-c-format
msgid ""
"Set this option to <userinput>false</userinput> to disable the colorful "
"output of &kdesrc-build;. Note that &kdesrc-build; will not output the color "
"codes to anything but a terminal (such as xterm, &konsole;, or the normal "
"&Linux; console)."
msgstr ""

#. Tag: para
#: conf-options-table.docbook:63
#, no-c-format
msgid "Related command-line option: <xref linkend=\"cmdline-color\"/>"
msgstr ""

#. Tag: link
#: conf-options-table.docbook:68
#, no-c-format
msgid "disable-agent-check"
msgstr ""

