#. Tag: para
#: man-plasmapkg2.1.docbook:43
#, no-c-format
msgid ""
"<command>plasmapkg2</command> is a command line tool to install, list, "
"remove &plasma; packages."
msgstr ""

#. Tag: title
#: man-plasmapkg2.1.docbook:49
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: term
#: man-plasmapkg2.1.docbook:53
#, no-c-format
msgid "<option>--hash</option> <replaceable> path</replaceable>"
msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:54
#, no-c-format
msgid "Generate a SHA1 hash for the package at <quote>path</quote>."
msgstr ""

#. Tag: option
#: man-plasmapkg2.1.docbook:57
#, no-c-format
msgid "-g, --global"
msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:58
#, no-c-format
msgid "For install or remove, operates on packages installed for all users."
msgstr ""

#. Tag: term
#: man-plasmapkg2.1.docbook:61
#, no-c-format
msgid "<option>-t, --type</option> <replaceable> type</replaceable>"
msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:62
#, no-c-format
msgid ""
"The type of package, &eg; theme, wallpaper, plasmoid, dataengine, runner, "
"layout-template, &etc; [plasmoid]."
msgstr ""

