#: man-plasmapkg2.1.docbook:77
#, no-c-format
msgid "-l, --list"
msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:78
#, no-c-format
msgid "List installed packages."
msgstr ""

#. Tag: option
#: man-plasmapkg2.1.docbook:81
#, no-c-format
msgid "--list-types"
msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:82
#, no-c-format
