msgstr ""

#. Tag: para
#: man-plasmapkg2.1.docbook:86
#, no-c-format
msgid "Remove the package named <quote>name</quote>."
msgstr ""

#. Tag: term
#: man-plasmapkg2.1.docbook:89
#, no-c-format
msgid "<option>-p, --packageroot</option> <replaceable> path</replaceable>"
msgstr ""

