#. Tag: option
#: index.docbook:1497
#, no-c-format
msgid "--show-tabbar"
msgstr ""

#. Tag: para
#: index.docbook:1498
#, no-c-format
msgid "<action>Show</action> the tabbar, overriding the default behavior."
msgstr ""

#. Tag: option
#: index.docbook:1503
#, no-c-format
msgid "--hide-tabbar"
msgstr ""

#. Tag: para
#: index.docbook:1504
#, no-c-format
msgid "<action>Hide</action> the tabbar, overriding the default behavior."
msgstr ""

#. Tag: option
#: index.docbook:1509
#, no-c-format
msgid "--fullscreen"
msgstr ""

#. Tag: para
#: index.docbook:1510
#, no-c-format
msgid "<action>Start &konsole; in fullscreen mode</action>."
msgstr ""

#. Tag: option
